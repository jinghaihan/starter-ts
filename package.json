{
  "name": "lib-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.18.3",
  "description": "",
  "author": "jinghaihan",
  "license": "MIT",
  "homepage": "https://github.com/jinghaihan/lib-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinghaihan/lib-name.git"
  },
  "bugs": {
    "url": "https://github.com/jinghaihan/lib-name/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
